<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:nameless="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scrollView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:id="@+id/translators"
        style="@style/settingsFragmentStyle">

        <com.android.settings.nameless.widgets.DeveloperPreference
            android:id="@+id/dummy_translator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            nameless:crowdinLink="@string/nameless_translators_crowdin"
            nameless:nameDev="@string/maintainers_nameless_name" />
    </LinearLayout>

</ScrollView>
